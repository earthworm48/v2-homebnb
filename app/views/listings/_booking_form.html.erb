<%= form_for @booking do |f| %>
  
  <div class="form-group">
    <%= f.label :start_date %><br>
    <%= f.text_field :start_date %>

  </div>
  <div class="form-group">
    <%= f.label :end_date %><br>
    <%= f.text_field :end_date %>
  </div>
  <div class="form-group">
    <%= f.label :no_of_guest %><br>
    <%= f.select :no_of_guest, (1..16), :style => "width:50%;"%>
  </div>




<% end %>
</form>